{
  "name": "relevel-me",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "worker",
    "web"
  ],
  "description": "Open-source gamified skill-tracking dashboard with voice-first journaling and immersive 3D worldboard interface",
  "keywords": [
    "skill-tracking",
    "gamification",
    "nextjs",
    "typescript",
    "supabase",
    "voice-ai",
    "self-hosted",
    "3d",
    "three-js",
    "react"
  ],
  "author": "relevel.me contributors",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishekslab/relevel.me.git"
  },
  "bugs": {
    "url": "https://github.com/abhishekslab/relevel.me/issues"
  },
  "homepage": "https://github.com/abhishekslab/relevel.me#readme",
  "scripts": {
    "dev": "cd web && npm run dev",
    "build": "cd web && npm run build",
    "start": "cd web && npm run start",
    "typecheck": "cd web && npm run typecheck",
    "worker:dev": "cd worker && npm run dev",
    "worker:build": "cd worker && npm run build",
    "worker:start": "cd worker && npm start"
  },
  "devDependencies": {
    "supabase": "^2.54.11",
    "tsx": "^4.20.6",
    "typescript": "5.6.2"
  }
}
